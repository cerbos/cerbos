      "get": {
        "summary": "List audit log entries",
        "operationId": "CerbosAdminService_ListAuditLogEntries",
        "responses": {
          "200": {
            "description": "A successful response.(streaming responses)",
            "schema": {
              "type": "object",
              "properties": {
                "result": {
                  "$ref": "#/definitions/v1ListAuditLogEntriesResponse"
